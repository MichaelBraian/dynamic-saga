# Supabase Configuration
VITE_SUPABASE_URL=your_supabase_url                    # e.g., https://xxxxxxxxxxxx.supabase.co
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key         # Public anon key
SUPABASE_SERVICE_ROLE_KEY=your_service_role_key        # Secret service role key (keep private)
SUPABASE_JWT_SECRET=your_jwt_secret                    # JWT secret for auth
SUPABASE_PROJECT_ID=your_project_id                    # Project ID from dashboard URL

# OpenAI Configuration
VITE_OPENAI_API_KEY=your_openai_api_key

# Application Settings
VITE_APP_NAME=Dynamic Saga
VITE_API_URL=http://localhost:3000
VITE_MAX_REQUESTS_PER_MINUTE=3

# Database Settings (if needed for direct connection)
SUPABASE_DB_HOST=your_db_host                         # e.g., db.xxxxxxxxxxxx.supabase.co
SUPABASE_DB_PORT=5432                                 # Default PostgreSQL port
SUPABASE_DB_NAME=postgres                             # Default database name
SUPABASE_DB_USER=postgres                             # Database user
SUPABASE_DB_PASSWORD=your_db_password                 # Database password 